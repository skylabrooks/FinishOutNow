{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_GEMINI_API_KEY": "@vite_gemini_api_key",
    "VITE_DALLAS_API_KEY_ID": "@vite_dallas_api_key_id",
    "VITE_DALLAS_API_KEY_SECRET": "@vite_dallas_api_key_secret"
  }
}
